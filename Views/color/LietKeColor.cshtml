@{
}
@model List<Color>

<h1>Danh sách color</h1>
<table border="1">
    <tr>
        <th>ID</th>
        <th>ColorName</th>
        <th>ColorImage</th>
    </tr>
    @foreach (var item in Model)
    {
<tr>
    <td>@item.Id</td>
    <td>@item.ColorName</td>
    <td>@item.ColorImage</td>
</tr>
    }


</table>
