@{
}

@{ ViewData["Tilte"] = "Danh sách sản phẩm"; }
@model IEnumerable<ShoesLover.Models.Product>
@{
                <table class="table">
                    <tr>
                        <th> Mã sản phẩm</th>
                        <th> Tên sản phẩm</th>
                    </tr>
                    @foreach (var item in Model)
                    {
                        <tr>
                            <td>@item.Id</td>
                            <td>@item.ProductName</td>
                        </tr>
                    }
                </table> }

