@{
}
@model List<Object>
<h2>Số sản phẩm </h2>
<table border="1">
    <tr>
        <th>Tên sản phẩm</th>
        <th>Màu sản phẩm</th>
        <th>Size sản phẩm</th>
    </tr>

    @foreach (var item in Model)
    {
<tr>
    <td>
        @item.GetType().GetProperty("productname").GetValue(item, null)
    </td>
    <td>
        @item.GetType().GetProperty("colorname").GetValue(item, null)
    </td>
    <td>
        @item.GetType().GetProperty("sizename").GetValue(item, null)
    </td>
</tr>
    }
</table>
