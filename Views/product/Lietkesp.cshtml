@{
}
@model List<Product>
<h1> Danh sách các sản phẩm</h1>
   
    <table border="1">
        <tr>
            <th>Tên sản phẩm</th>
            <th>Giá bình thường </th>
            <th>Giá sale</th>
            <th>Mô tả</th>
        </tr>
       @foreach( var item in Model){
        <tr>
            <td>@item.ProductName</td>
            <td>@item.RegularPrice</td>
            <td>@item.SalePrice</td>
            <td>@item.Description</td>
            <td><a></a></td>
            
        </tr>

        }

    </table>

