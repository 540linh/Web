@model List<Size>
    <h2>Danh sách size</h2>
<form method="post" action="product/FilterSPTheoSize">

    
   
        <label for="SizeName">Bộ lọc theo size</label>
        <select name="ID" id="SizeName">
            @foreach (var item in Model)
            {
                <option value="@item.Id">@item.SizeName</option>

            }
        </select><br>
        <input type="submit" name="submit" value="Liệt kê" />
    </form>
